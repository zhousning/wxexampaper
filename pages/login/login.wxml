      <view class="cu-form-group">
        <view class="title">密码</view>
        <input placeholder="填写密码" name="password" type="password" bindinput="passwordInput" />
      </view>

      <view class="padding flex flex-direction">
        <button class="cu-btn bg-blue margin-tb-sm lg" bindtap="login">登 录</button>
      </view>
    </view>
  </form>
